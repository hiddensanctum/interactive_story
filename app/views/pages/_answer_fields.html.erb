<fieldset>
  <%= f.label :image %><br>
  <%= f.file_field :image %>
  <%= f.text_field :name, class:"form-control", placeholder: "write answer here" %>
  <%= f.hidden_field :_destroy %>
  <%= f.check_box :correct %>
  <%= f.label :correct, "correct answer?"%>
  <%= link_to "remove", '#', class: "remove_fields" %>
</fieldset>

